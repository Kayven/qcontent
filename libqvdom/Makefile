first: all
####### Implicit rules

.SUFFIXES: .c .o .cpp .cc .cxx .C

.cpp.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.cc.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.cxx.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.C.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.c.o:
	$(CC) -c $(CFLAGS) $(INCPATH) -o $@ $<

####### Build rules

all: $(TARGET)

$(TARGET):  $(UICDECLS) $(OBJECTS) $(OBJMOC) $(SUBLIBS) $(OBJCOMP)  
	-$(DEL_FILE) $(TARGET) $(TARGET0) $(TARGET1) $(TARGET2)
	$(LINK) $(LFLAGS) -o $(TARGET) $(OBJECTS) $(OBJMOC) $(LIBS) $(OBJCOMP)
	-ln -s $(TARGET) $(TARGET0)
	-ln -s $(TARGET) $(TARGET1)
	-ln -s $(TARGET) $(TARGET2)



staticlib: $(TARGETA)

$(TARGETA):  $(UICDECLS) $(OBJECTS) $(OBJMOC) $(OBJCOMP)
	-$(DEL_FILE) $(TARGETA) 
	$(AR) $(TARGETA) $(OBJECTS) $(OBJMOC)

mocables: $(SRCMOC)
uicables: $(UICDECLS) $(UICIMPLS)

$(MOC): 
	( cd $(QTDIR)/src/moc && $(MAKE) )

dist: 
	@mkdir -p .tmp/vdom && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS)  .tmp/vdom/ && ( cd `dirname .tmp/vdom` && $(TAR) vdom.tar vdom && $(GZIP) vdom.tar ) && $(MOVE) `dirname .tmp/vdom`/vdom.tar.gz . && $(DEL_FILE) -r .tmp/vdom

mocclean:

uiclean:

yaccclean:
lexclean:
clean:
	-$(DEL_FILE) $(OBJECTS)
	-$(DEL_FILE) *~ core *.core


####### Sub-libraries

distclean: clean
	-$(DEL_FILE) $(TARGET) $(TARGET)
	-$(DEL_FILE) $(TARGET0) $(TARGET1) $(TARGET2) $(TARGETA)


FORCE:

####### Compile

vdom.o: vdom.cpp vdom.h

####### Install

install_headers: all 
	@$(CHK_DIR_EXISTS) "$(INSTALL_ROOT)/opt/qcontent/include/" || $(MKDIR) "$(INSTALL_ROOT)/opt/qcontent/include/"
	-$(INSTALL_FILE) "vdom.h" "$(INSTALL_ROOT)/opt/qcontent/include/"


uninstall_headers: 
	-$(DEL_FILE) -r "$(INSTALL_ROOT)/opt/qcontent/include/vdom.h"
	-$(DEL_DIR) "$(INSTALL_ROOT)/opt/qcontent/include/"


install_target: all 
	@$(CHK_DIR_EXISTS) "$(INSTALL_ROOT)/opt/qcontent/lib/" || $(MKDIR) "$(INSTALL_ROOT)/opt/qcontent/lib/"
	-$(INSTALL_FILE) "$(TARGET)" "$(INSTALL_ROOT)/opt/qcontent/lib/$(TARGET)"
	-$(SYMLINK) "$(TARGET)" "$(INSTALL_ROOT)/opt/qcontent/lib/$(TARGET0)"
	-$(SYMLINK) "$(TARGET)" "$(INSTALL_ROOT)/opt/qcontent/lib/$(TARGET1)"
	-$(SYMLINK) "$(TARGET)" "$(INSTALL_ROOT)/opt/qcontent/lib/$(TARGET2)"

uninstall_target: 
	-$(DEL_FILE) "$(INSTALL_ROOT)/opt/qcontent/lib/$(TARGET)"
	-$(DEL_FILE) "$(INSTALL_ROOT)/opt/qcontent/lib/$(TARGET0)"
	-$(DEL_FILE) "$(INSTALL_ROOT)/opt/qcontent/lib/$(TARGET1)"
	-$(DEL_FILE) "$(INSTALL_ROOT)/opt/qcontent/lib/$(TARGET2)"
	-$(DEL_DIR) "$(INSTALL_ROOT)/opt/qcontent/lib/"


install: install_headers install_target  

uninstall: uninstall_headers uninstall_target  

